<div>
  random section
  <div>title:{{ section.settings.title_1 }}</div>
  <div>title:{{ section.settings.title_2 }}</div>

  <div>
        <img class="w-100 d-block" src="{{ section.settings.image | image_url: width: 1100}}"
    </div>
</div>

{% schema %}
{
  "name": "Random Section",
  "tag": "section",
  "settings": [
    {
      "id": "title_1",
      "type": "textarea",
      "label": "Title 1"
    },
    {
      "id": "title_2",
      "type": "textarea",
      "label": "Title 2"
    },
    {
      "id": "image",
      "type": "image_picker",
      "label": "Image"
    }
  ],
  "presets": [
    {
      "name": "Random Section"
    }
  ]
}
{% endschema %}
